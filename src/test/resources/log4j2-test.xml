<?xml version="1.0" encoding="UTF-8"?>
<configuration status="ON">
  <appenders>
    <Console name="Console" target="SYSTEM_OUT">
      <PatternLayout pattern="%6r [seq%5sn] [%t] %-5level %-20c{1} - %msg%n" />
    </Console>
  </appenders>
  <Loggers>
    <Root level="info">
      <AppenderRef ref="Console" />
    </Root>
  </Loggers>
</configuration>
